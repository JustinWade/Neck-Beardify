{
"manifest_version": 2,
    "name": "Neckbeardify",
    "description": "Changes your everyday boring words into glorious neckbeard phrases.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}